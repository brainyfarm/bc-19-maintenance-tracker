{
  "name": "bc-19-maintenance-tracker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/nodemon ./bin/www",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha test/**/*.spec.js",
    "test-unit": "NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.spec.js",
    "test-integration": "NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.spec.js",
    "db-init": "jake db:init && NODE_ENV=test jake db:init",
    "db-drop": "jake db:drop && NODE_ENV=test jake db:drop",
    "db-migrate": "./node_modules/.bin/sequelize db:migrate && ./node_modules/.bin/sequelize db:migrate --env test"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.24.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "jake": "latest",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "sequelize-cli": "^2.4.0",
    "supertest": "^2.0.0"
  }
}
